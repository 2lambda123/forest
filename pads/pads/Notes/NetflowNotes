  /* read header : 16 bytes */

   2  bytes : version
   2  bytes : count

   4  bytes : uptime

   4  bytes : sec

   4  bytes : usec

  /* read data :  44 bytes */

   4  bytes : src_addr 

   4  bytes : dst_addr 

   4  bytes : hop 

   2  bytes : input 
   2  bytes : output 

   4  bytes : packets 

   4  bytes : bytes 

   4  bytes : firt 

   4  bytes : last 

   2  bytes : src_port 
   2  bytes : dst_port 

   1  bytes : flags        | flags/tcp_flags
   1  bytes : tcp_flags    | together 'cover' r1->pad1
   1  bytes : prot
   1  bytes : tos

   /* skip over (do not fill in): src_as, dst_as, src_mask, dst_mask, pad2, router_sc */

   1  bytes : pad1 /* r1->flags */
   1  bytes : tcp_retx_cnt
   1  bytes : tcp_retx_secs
   1  bytes : tcp_misseq_cnt

[   4  bytes : pad2  ]

   16 + 44     = 60 bytes
   add in pad2 = 64 bytes

 Mods to abstract C-struct (no on-disk bytes):
      flags     = r1->flags /* copy flags to flags spot in canon struct */
      memcpy(&rp->pad1, &r1->flags, 4) /* move down 4 bytes of data to the *V1* slot of canonical rep */ 
                                       /* 

      /* zero out fields that were not filled in */
      src_as    = 0
      dst_as    = 0
      src_mask  = 0
      dst_mask  = 0
      pad2      = 0
      router_sc = 0  

