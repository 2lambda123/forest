\begin{code}
\kw{typedef} \kw{struct}  \{
  Puint32\_acc nerr;
  order\_header\_t\_acc h;
  eventSeq\_t\_acc events;
\} entry\_t\_acc;
\mbox{}
Perror\_t entry\_t\_acc\_init (P\_t *pads,entry\_t\_acc *acc);
Perror\_t entry\_t\_acc\_reset (P\_t *pads,entry\_t\_acc *acc);
Perror\_t entry\_t\_acc\_cleanup (P\_t *pads,entry\_t\_acc *acc);
Perror\_t entry\_t\_acc\_add (P\_t *pads,entry\_t\_acc *acc,
                          entry\_t\_pd *pd,entry\_t *rep);
Perror\_t entry\_t\_acc\_report2io (P\_t *pads,Sfio\_t *outstr,\kw{char} \kw{const} *prefix,
                                \kw{char} \kw{const} *what,\kw{int} nst,entry\_t\_acc *acc);
Perror\_t entry\_t\_acc\_report (P\_t *pads,\kw{char} \kw{const} *prefix,
                             \kw{char} \kw{const} *what,
                             \kw{int} nst,entry\_t\_acc *acc);
\end{code}
