## Three columns seperated by white spaces
#TYPE	NAME		regex
def	triplet		[0-9]{1,3}
def doublet		[0-9]{2}
def	hexdoublet	[0-9a-fA-F]{2}
def	timezone 	[+-][0-1][0-9]00
def	ampm 		am|AM|pm|PM
def	port		[1-9][0-9]*
def	filename	[^][\/\\?*:<>+"']+
def	day		[1-9]|[1-2][0-9]|0[1-9]|3[0-1]
def	weekday		Mon|Monday|Tue|Tuesday|Wed|Wednesday|Thu|Thursday|Fri|Friday|Sat|Saturday|Sun|Sunday|mon|tue|wed|thu|fri|sat|sun
def	month		Jan|jan|Feb|feb|Mar|mar|Apr|apr|May|may|Jun|jun|Jul|jul|Aug|aug|Sep|sep|Oct|oct|Nov|nov|Dec|dec|January|February|March|April|May|June|July|August|September|October|November|December
def	nummonth	0?[1-9]|1[0-2]
def genmonth	{month}|{nummonth}
def	domainsuffix	com|net|edu|org|gov
def	year		[0-2][0-9]{3}
def str		[A-Za-z][A-Za-z0-9_-]*
def str1		[0-9A-Za-z][A-Za-z0-9_-]*
#def hostlabel
#[0-9A-Za-z]|[0-9A-Za-z][0-9A-Za-z]|[0-9A-Za-z][A-Za-z0-9_\-]{1,61}[0-9A-Za-z]
def hostlabel   [0-9A-Za-z_-]{1,63}
def	query		[^&=]+=[^&]*(\&[^&=]+=[^&]*)*\&?
def	username	[-a-zA-Z0-9!#$%&'*+/=?\^_`{|}~][-a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]*[-a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]|[-a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]
def	hostname    ({hostlabel}\.)+({domainsuffix}|{domainsuffix}\.[a-z][a-z])
#def hostname    {hostlabel}(\.{hostlabel})+
def sysname		({hostlabel}\.)+{hostlabel}
def emaillocal  ([0-9A-Za-z!#$%&'*+/=?\^_`{|}~]|(\-))([.]?([a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]|(\-))){1,61}([a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]|(\-))|([a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]|(\-))|([a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]|(\-))([a-zA-Z0-9!#$%&'*+/=?\^_`{|}~]|(\-)) 
def	protocol	http|ftp|https
def nums        [0-9]+
exp Pint        [-~]?{nums}
exp Pfloat      {Pint}|[-~]?{nums}\.{nums}|[-~]?[1-9]\.{nums}(E|e)[-~]?{nums}
exp	Ptime    	{doublet}:{doublet}:{doublet}([ ]*{ampm})?([ \t]+{timezone})?|{doublet}:{doublet}
exp Pdate       {genmonth}\/{day}\/{year}|{day}\/{genmonth}\/{year}|{year}\/{genmonth}\/{day}|{genmonth}\-{day}\-{year}|{day}\-{genmonth}\-{year}|{year}\-{genmonth}\-{day}|{genmonth}\.{day}\.{year}|{day}\.{genmonth}\.{year}|{year}\.{genmonth}\.{day}|({weekday},?[ \t]+)?{month}[ \t]+{day}(,[ \t]+{year})?|({weekday},?[ \t]+)?{day}[ \t]+{month}(,[ \t]+{year})?
exp	Pip         {triplet}\.{triplet}\.{triplet}\.{triplet}
exp	Phostname	{hostname}
exp	Pemail		{emaillocal}@{sysname}
exp	Pmac		({hexdoublet}(:|\-)){5}{hexdoublet}
#exp	Ppath
#(\/{filename}){2}(\/{filename})*\/?|({filename}\/){2}({filename}\/)*{filename}?|\\?(\\{filename}){2}(\\{filename})*\\?|({fi#lename}\\){2}({filename}\\)*{filename}?|{filename}|\/{filename}|\\{filename}|\/|\\
exp	Ppath		(\/{filename})(\/{filename})+\/?|\\?(\\{filename})(\\{filename})+\\?
#|({filename}\\)({filename}\\)*{filename}?|({filename}\/)({filename}\/)*{filename}?
exp	Purl        {protocol}:\/\/{sysname}(:{port})?\/?(\/{filename})*\/?(\?)?\&?{query}?(#{str1})?|{protocol}:\/\/{Pip}(:{port})?\/?(\/{filename})*\/?(\?)?\&?{query}?(#{str1})?
exp Purlbody    {sysname}(:{port})?\/?(\/{filename})*\/?(\?)?\&?{query}?(#{str1})?|\/\/{Pip}(:{port})?\/?(\/{filename})*\/?(\?)?\&?{query}?(#{str1})?
exp Pword       [-A-Za-z']+
exp Pid         [-0-9A-Za-z_.]+
exp	PbXML		\<([a-zA-Z])+\>
def	oxmlb		\<[^>]+\>
exp	PeXML		\<\/[^>]+\>
exp	Pwhite		[ \t\r\n]+
exp Pmessage    \"[^"]+\"|[[][^]]+[]]|[(][^)]+[)]|[{][^}]+[}]|[^?]+\?|:[^:]+  
exp Ppermission [-dl]([-r][-w][-xsStT]){3}
exp Ptext       [\"'][-A-Za-z0-9_,;. ]+[\"']|[-A-Za-z0-9_,;. ]
exp Phstring    [0-9a-f]+
exp Ppunc	[.\/\\;|<>~`!@#$%\^&*()-_+={}[]:"',?]
exp Ppunc_dot        [.]
exp Ppunc_slash      [\/]
exp Ppunc_bslash     [\\]
exp Ppunc_scolon     [;]
exp Ppunc_bar        [|]
exp Ppunc_less       [<]
exp Ppunc_tilde      [~]
exp Ppunc_bquote     [`]
exp Ppunc_bang       [!]
exp Ppunc_at         [@]
exp Ppunc_hash       [#]
exp Ppunc_dollar     [$]
exp Ppunc_percent    [%]
exp Ppunc_caret      [\^]
exp Ppunc_and        [&]
exp Ppunc_star       [*]
exp Ppunc_lpar       [(]
exp Ppunc_rpar       [)]
exp Ppunc_hyphen     [-]
exp Ppunc_underscore [_]
exp Ppunc_plus       [+]
exp Ppunc_equa       [=]
exp Ppunc_lbrac      [{]
exp Ppunc_rbrac      [}]
exp Ppunc_lsqubrac   [[]
exp Ppunc_rsqubrac   []]
exp Ppunc_colon      [:]
exp Ppunc_dquote     ["]
exp Ppunc_quote      [']
exp Ppunc_greater    [>]
exp Ppunc_comma      [,]
exp Ppunc_question   [?]
#exp Pblob       ^{Pint}{Pfloat}{Ptime}{Pdate}{Pip}{Phostname}{Pemail}{Pmac}{Ppath}{Purl}{Purlbody}{Pword}{Pid}{PeXML}{PbXML}{Pwhite}{Pmessage}{Ppermission}{Pdot}{Pslash}{Pbackslash}{Psemicolon}{Pbar}{Pless}{Ptilde}{Pbquote}{Pbang}{Pat}{Phash}{Pdollar}{Ppercent}{Pcaret}{Pand}{Pstar}{Plpar}{Prpar}{Pdash}{Punderscore}{Pplus}{Pequa}{Plbrac}{Prbrac}{Plsqubrac}{Prsqubrac}{Pcolon}{Pdquote}{Pquote}{Pgreater}{Pcomma}{Pquestion}
