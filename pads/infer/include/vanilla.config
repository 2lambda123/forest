## Three columns seperated by white spaces
#TYPE	NAME		regex
def	triplet		[0-9]{1,3}
def 	doublet		[0-9]{2}
def	hexdoublet	[0-9a-fA-F]{2}
def	timezone 	[+-][0-1][0-9]00
def	ampm 		am|AM|pm|PM
def	port		[1-9][0-9]*
def	day		[1-9]|[1-2][0-9]|0[1-9]|3[0-1]
def	weekday		Mon|Monday|Tue|Tuesday|Wed|Wednesday|Thu|Thursday|Fri|Friday|Sat|Saturday|Sun|Sunday|mon|tue|wed|thu|fri|sat|sun
def	month		Jan|jan|Feb|feb|Mar|mar|Apr|apr|May|may|Jun|jun|Jul|jul|Aug|aug|Sep|sep|Oct|oct|Nov|nov|Dec|dec|January|February|March|April|May|June|July|August|September|October|November|December
def	nummonth	0?[1-9]|1[0-2]
def 	genmonth	{month}|{nummonth}
def	domainsuffix	com|net|edu|org|gov|co|ac|mil|info|int
def	year		[0-2][0-9]{3}
def	quote		["']
def 	str		[A-Za-z][A-Za-z0-9_\-]*
def 	str1		[0-9A-Za-z_][A-Za-z0-9_\-]*
def	filename	[0-9A-Za-z_.][A-Za-z0-9_\-.*%'&]*
def	query		{str1}=[0-9a-zA-Z_\-%;.+/:]*(\&{str1}=[0-9a-zA-Z_\-%;.+/:]*)*\&?
def	username	[0-9a-zA-Z\-_+][0-9a-zA-Z\-_+.]*
#def	hostname	{str1}+\.{domainsuffix}(\.[a-zA-Z][a-zA-Z])?|({str1}\.)+[a-zA-Z][a-zA-Z]+
def	hostname	({str1}\.)+[a-zA-Z][a-zA-Z]+
def 	sysname		({str1}\.)+{str1}
def	protocol	http|ftp|https
def	safeurlchar	[^<>{}|\^\[\] \t\n\r"()']
exp	Ptime    	{doublet}:{doublet}:{doublet}([ ]*{ampm})?([ \t]+{timezone})?
exp	Pip		{triplet}\.{triplet}\.{triplet}\.{triplet}
exp	Pemail		{username}@{sysname}
exp	Pmac		({hexdoublet}(:|\-)){5}{hexdoublet}
exp   Pdate           {genmonth}\/{day}\/{year}|{day}\/{genmonth}\/{year}|{year}\/{genmonth}\/{day}|{genmonth}\-{day}\-{year}|{day}\-{genmonth}\-{year}|{year}\-{genmonth}\-{day}|{genmonth}\.{day}\.{year}|{day}\.{genmonth}\.{year}|{year}\.{genmonth}\.{day}|({weekday},?[ \t]+)?{month}[ \t]+{day}(,[ \t]+{year})?|({weekday},?[ \t]+)?{day}[ \t]+{month}(,[ \t]+{year})?
exp	Ppath		\/{filename}?\/{filename}?(\/{filename}?)*\/?|{filename}\/{filename}?\/({filename}?\/)*{filename}?|\\{filename}?\\{filename}?(\\{filename}?)*\\?|{filename}\\{filename}?\\({filename}?\\)*{filename}?|{protocol}:\/\/({sysname}|{str1})(:{port})?\/?{safeurlchar}*|(\/{filename})+\;{str}={str}\?{safeurlchar}*|(\/{filename})+\;{str}={str}|(\/{filename})+\/?\?{safeurlchar}*
#exp	Purl		{protocol}:\/\/{sysname}(:{port})?\/?(\/{filename})*\/?(\;{str}={str})?(\?)?\&?{query}?(#{str1})?|{protocol}:\/\/{Pip}(:{port})?\/?(\/{filename})*\/?(\;{str}={str})?(\?)?\&?{query}?(#{str1})?|(\/{filename})+(\;{str}={str})?\?{query}
#exp	Purl		{protocol}:\/\/{sysname}(:{port})?\/?[^ \t\n\r"()']*|(\/{filename})+\;{str}={str}\?[^ \t\n\r"()']*|(\/{filename})+\;{str}={str}|(\/{filename})+\?[^ \t\n\r"()']*
exp	Phostname	{hostname}
exp	PbXML		\<([a-zA-Z])+\>
def	oxmlb		\<[^>]+\>
exp	PeXML		\<\/[^>]+\>
exp	Pwhite		[ \t\r\n]+
exp 	Pint		[0-9]+
exp	Pstring		{str}
#exp 	Ptext		{quote}[ .]*[A-Za-z0-9_\-]+([,;. -]+[A-Za-z0-9_\-]+)*([,;. -]+)?{quote}
